    "@types/file-saver": "^2.0.5",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-table": "^7.7.14",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",

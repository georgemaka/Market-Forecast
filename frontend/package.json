  "version": "1.0.0",
  "description": "Frontend React app for Sukut Construction Market Forecasting Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
